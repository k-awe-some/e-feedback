    "express": "^4.17.1",
    "mongoose": "^5.7.5",
    "nodemon": "^1.19.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "2",
    "sendgrid": "^5.2.3",
    "stripe": "^7.9.1"
  }
}

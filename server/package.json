{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "10.16.0",
    "yarn": "1.16.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
